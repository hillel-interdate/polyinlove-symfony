<script>
    $(window).load(function () {
        // setTimeout(function () {
        var html = '<ul class="clearfix">';
        {% for article in articles %}
        {% set article_url = path("magazine_article", {"uri": article.uri }) %}
        html += '<li><a href="';
        html += '{{ article_url }}';
        html += '"><div class="imagediv">' +
            '<img class="main-articles-images" src="{{ article.imageName }}" alt="{{ article.imageAlt }}">' +
            '</div></a>' +
            '<a href="{{ article_url }}" class="readmore"><h3>{{ article.name }}</h3></a>' +
            '<p>';
        {% if article.brief|length > 127 %}
        html += '{{ article.brief|slice(0,126) }}...';
        {% else %}
        html += '{{ article.brief }}';
        {% endif %}
        html += '</p><a href="{{ article_url }}" class="readmore">{% trans %} Read more {% endtrans %}</a>' +
            '</li>';
        {% endfor %}
        html += '</ul>';
        $('.hp-articles .wrap .heading').after(html);

    });
</script>
