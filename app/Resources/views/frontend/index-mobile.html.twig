{% set footerBlocks = getFooterBlocks() %}

<footer>
    <div class="wrap clearfix">
        {% for block in footerBlocks %}
            <div class="menudiv{{ loop.index }}">
                <h4>{{ block.name }}</h4>
                <ul>
                    {% for page in block.pages %}
                        {% if page.isActive %}
                            <li><a href="{{ path('pages_page', {'uri': page.uri}) }}">{{ page.name }}</a></li>
                        {% endif %}
                    {% endfor %}
                    {% if loop.index == 1 %}
                        {% if not app.user %}
                            <li><a href="{{ path('users_public_list') }}">חברים באתר</a></li>
                        {% endif %}
                    {% endif %}
                    {% if loop.index == 3 %}
                        <li><a href="{{ path('faq') }}">{% trans %} Faq {% endtrans %}</a></li>
                    {% endif %}
                </ul>
            </div>
        {% endfor %}

        <div class="leftsect">
            <div class="topsec clearfix">
                <div class="social-icon clearfix">
                    <h5>Join us!</h5>
                    <a href="{{ facebook_group_link }}" class="social-fb" target="_blank"><img
                                src="/images/footer-facebook-icon.png" alt="{{ site_name }} | polyamory"></a>
                    <a href="{{ instagram_link }}" class="social-google" target="_blank"><img
                                src="/images/footer-insta-icon.png" alt="{{ site_name }} | polyamory"></a>
                </div>
                <div class="email-icon social-icon">
                    <h5>{% trans %} Contact Us {% endtrans %}</h5>
                    <a href="{{ path('contact') }}"><img src="/images/footer-contact-icon.png"
                                                         alt="{{ site_name }} | polyamory"></a>
                </div>
                <div class="facebook-row">
                    <img src="/images/footer-facebook.png" alt="{{ site_name }} | polyamory" onclick="checkLoginState()">
                    <a href="{{ path('sign_up') }}">
                        <button class="footer-free-signup"> {% trans %} Free sign up {% endtrans %}</button>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <div class="wrap2"><a class="scroltop" href="#"></a></div>
</footer>
